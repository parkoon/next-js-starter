{
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@": "./src",
          "@components": "./src/components",
          "@container": "./src/container",
          "@helpers": "./src/helpers",
          "@state": "./src/state",
          "@server": "./server"
        }
      }
    ],
    [
      "styled-components",
      {
        "ssr": true
      }
    ]
  ]
}
